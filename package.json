{
  "name": "monopoly",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "client:dev": "npm run dev --prefix client",
    "client:build": "npm run build --prefix client",
    "client:preview": "npm run preview --prefix client",
    "server:dev": "npm run dev --prefix server",
    "server:start": "npm start --prefix server",
    "dev": "concurrently -n client,server \"npm:client:dev\" \"npm:server:dev\""
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
